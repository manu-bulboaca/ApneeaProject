app.controller('graphsController', function($rootScope, $scope, $http,
		serviceHttp) {

	serviceHttp.post("/userTST_"+"0001", '[{"id": "' + $scope.desc
			+ '", "user_id": "' + $scope.userSelected._id + '", "user_name": "'
			+ $scope.userSelected.name + '", "machine_id": "' + $scope.machine
			+ '"}]', function(json) {
		console.log(json)
	})

});
