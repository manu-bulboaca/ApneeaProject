 */
app.controller('homeController', function($rootScope, $scope, $http, serviceHttp){
	serviceHttp.get("/usersTST",function(json){
		console.log(json.length);
		for (var i =0;i<json.length;i++){
			console.log(json[i]);
			$scope.users = json[i];
			console.log("USER "+$scope.users[i].name)
		}
	});
});
